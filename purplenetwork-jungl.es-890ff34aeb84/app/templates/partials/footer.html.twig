<div class="contact__footer">
						<div class="footer__main">
							<div class="footer__container">
								<div class="footer__desc">
								{% include 'partials/logo.html.twig'  with {'className':  'footer__logo'} %}
								<span class="footer_desc_cont">{% trans %}sections.footer.description{% endtrans %}</span>
								</div>
							<nav class="footer__resr js-main-menu-footer">
								<h3>{% trans %}sections.footer.resources_title{% endtrans %}</h3>
								<a href="#" data-id="study">Study</a>
								<a href="#" data-id="think">Think</a>
								<a href="#" data-id="move">Move</a>
								<a href="#" data-id="connect">Connect</a>
								<a href="#" data-id="contact">Contact Us</a>
							</nav>
							<div class="footer__support">
								<h3>{% trans %}sections.footer.support_title{% endtrans %}</h3>
								<a href="{{ path('privacy_site') }}" target="_blank" >{% trans %}sections.footer.privacy_policy{% endtrans %}</a>
								<a href="mailto:{% trans %}sections.footer.support_email{% endtrans %}">{% trans %}sections.footer.support_email{% endtrans %}</a>
								<a href="tel:{% trans %}sections.footer.support_tel{% endtrans %}">{% trans %}sections.footer.support_tel{% endtrans %}</a>
							</div>
							<div class="footer__follow">
                                <h3>{% trans %}sections.footer.follow_title{% endtrans %}</h3>
                                <div class="footer__icons">
                            	    <a href="https://www.linkedin.com/company/28876290">{% include 'svg/linkedin.html.twig' %}</a>
                            	    <a href="https://www.facebook.com/jungles.experience">{% include 'svg/facebook.html.twig' %}</a>
                            	    <a href="https://www.instagram.com/jungles_ux/">{% include 'svg/instagram.html.twig' %}</a>
                            	    <a href="https://www.youtube.com/channel/UCleFADL-b2zGeHj0TIlOAzg">{% include 'svg/youtube.html.twig' %}</a>
                                </div>
                            </div>
							</div>
						</div>
						<div class="footer__sub">
							<div class="footer__container">
                                <span class="footer__spacerafter">
								    <span class="footer__dotafter">{% trans %}sections.footer.copyright{% endtrans %}</span>
								    <span class="footer__dotafter">{% trans %}sections.footer.address{% endtrans %}</span>
								    <span>{% trans %}sections.footer.piva{% endtrans %}</span>
                                </span>
                                <div class="footer__lang">
                                    {% include 'svg/globe.html.twig' %}
                                    {% trans %}sections.footer.lang{% endtrans %}
                                </div>
							</div>
						</div>
					</div>